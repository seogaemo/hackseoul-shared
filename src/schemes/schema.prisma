generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  UNSPECIFIED // 기본값 또는 알 수 없는 역할
  ADMIN // 관리자
  DISTRIBUTOR // 위탁판매자
}

model User {
  uid      String @id @default(uuid())
  name     String
  id       String
  password String
  role     Role
}
